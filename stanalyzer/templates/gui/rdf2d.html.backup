<script type="text/javascript">
var $j = jQuery.noConflict();
var mySystem;		// this gloabl value containing system information defined in 'index.html' when $j('.next-tab').click() is trigged!


/* ##########################################
 * Main function starts
 * ########################################## */

var cnt_RDF2D = 0; // total number of options in this module  

$j(document).ready(function(){
  // *** Hidden processdure: Read system information from index.html when $j('.next-tab').click(function(){}); is triggered

/*----------------------------------------------------
 * Control events
 * ---------------------------------------------------*/
    // Hide options at first until the module is checked
    $j("#C_RDF2D #M_RDF2D #RDF2D2").hide();
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D").hide();
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid").hide();
    
    
    // defining default value of initial forms
    $j('#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_frmInt').val('1').css("color", "gray");
    $j('#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_outFile').val('density_2d_lipid.dat').css("color", "gray");
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_hist_bin").empty().val('1').css("color", "gray");
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #txt_memb").empty().val("Define 'segid' or 'resname' to be centered! (e.g. segid MEMB)");
    $j('#C_RDF2D #M_RDF2D #RDF2D2 #cnt_radio_RDF2D_no').attr('checked', 'checked');
    $j('#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_single').attr('checked', 'checked');
    

    // Show sub menus
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #chk_RDF2D_lipid").live("click", function() {
	$j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid").toggle(this.checked);
    });
    
    
    //---> Add count to total number of selected modules
    $j("#C_RDF2D #M_RDF2D #RDF2D1 #RDF2D_title").live("click", function() {
	$j("#vfy_msg").empty();
	var chk = $j(this).prop('checked');
	if (chk == true) {
	    /* ----- reading system information ----- */
	    // Reading total number of atoms
	    var num_total_atoms = $j("#step2 #num_atoms").text();	// total number of atoms in the system
	    var num_files = $j("#step2 #num_files").text();		// total number of selected trajectory files 
	    var frames_per_trj = $j("#step2 #num_frame").text();	// number of frames per a trajectory file (e.g. each DCD file)
	    var ps_per_frame = $j("#step2 #num_ps").text();		// duration of simulation for each frame in picoseconds
	    
	    // total number of atoms are required for parameter passing
	    $j("#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_hist_tatoms").empty().val(num_total_atoms);

	} else {
	    $cnt_modules--;
	    cnt_RDF2D--;
	    if ($cnt_modules < 0) {
		$cnt_modules = 0;
	    }
    	    if (cnt_RDF2D < 0) {
		cnt_RDF2D = 0;
	    }
	}
	$j("#C_RDF2D #M_RDF2D #RDF2D2").toggle(this.checked);
	
	// Get system information
	var query = 'all';
	var bpath = $j("#proot option:selected").text();
	var stfile = $j("#stfile option:selected").text();
	var pdbfile = $j("#pdbfile option:selected").text();
	$j("#select_trj2 option").attr("selected", "selected");
	var trjFile = $j("#select_trj2").val();
	
	var stInfo = [];
	var systemInfo = {
	    'all' 		: stInfo,
	}
	var sndData = {
	    'cmd'       : 'getStructure',
	    'query'	    : query,
	    'bpath'	    : bpath,
	    'stfile'    : stfile,
	    'pdbfile'   : pdbfile,
	    'trjFile[]' : trjFile,
	}
    
	var request = $j.ajax({
	type: "POST",
	url: '/gui/getStructureInfo/',
	cache: false,
	data: sndData,
	async: true,
	});
		    
	request.done(function(Jdata) {
	    var obj = $j.parseJSON(Jdata);
	    var key_words_tmp = $.merge([], key_words);		// duplicate first array to prevent changing original array
	    stInfo = $j.merge(key_words_tmp, obj.uq_resname); 	// unqiue residue name
	    stInfo = $j.merge(stInfo, obj.uq_resid);		// unique residue IDs
	    stInfo = $j.merge(stInfo, obj.uq_name);		// unique atom names
	    stInfo = $j.merge(stInfo, obj.uq_segid);		// unique segment IDs
	    stInfo = $j.merge(stInfo, obj.uq_type);		// unique types
	    systemInfo['all']     = stInfo;
	    systemInfo['resname'] = obj.uq_resname;
	    systemInfo['resid']   = obj.uq_resid;
	    systemInfo['name']    = obj.uq_name;
	    systemInfo['segid']   = obj.uq_segid;
	    systemInfo['type']    = obj.uq_type;
	    systemInfo['crd_min'] = obj.crd_min;
	    systemInfo['crd_max'] = obj.crd_max;
	    systemInfo['num_atoms'] = obj.num_atoms;
	    systemInfo['size_x'] = obj.size_x;
	    systemInfo['size_y'] = obj.size_y;
	    systemInfo['size_z'] = obj.size_y;
	    
	    /* fill out the information about density min and max */
	    var smin = 0;
	    var smax = 0;
	    if (Math.abs(obj.crd_min) > Math.abs(obj.crd_max)) {
		smin = obj.crd_min;
		smax = Math.abs(obj.crd_min);
	    } else {
		smin = -1 * obj.crd_max;
		smax = obj.crd_max;
	    }
	    var size_x = obj.size_x;
	    var size_y = obj.size_y;
	    var size_z = obj.size_z;
	    
	    $j("#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_sizeX").empty().append(size_x);
	    $j("#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_sizeY").empty().append(size_y);
	    $j("#C_RDF2D #M_RDF2D #RDF2D2 #RDF2D_sizeZ").empty().append(size_z);
	});
	  
        request.error(function(jqXHR, textStatus, errorThrown) {
            $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #vfy_msg").empty().append(errorThrown);
        });

	
    });
    
    //---> show/hide the centering form upon the event of clicking yes/no radio button
    $("#C_RDF2D #M_RDF2D #RDF2D2 input:radio[name=cnt_radio_RDF2D]").each(function(i){
	$j(this).click(function() {
	    if (i == 0) {
		$j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #txt_memb").removeAttr("disabled");
		$j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D").show();
	    }
	    else {
		$j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #txt_memb").attr("disabled", "disabled");
		$j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D").hide();
	    }
	});
    });
    
    //---> Verification of selection query in the centering form
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #memb_btn_vfy").click(function(){
	var query = $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #txt_memb").val().trim();
	var bpath = $j("#proot option:selected").text().trim();
	var stfile = $j("#stfile option:selected").text().trim();
	var pdbfile = $j("#pdbfile option:selected").text().trim();
	$j("#select_trj2 option").attr("selected", "selected"); 	// select all
	var trjFile = $j("#select_trj2").val();
	var vfy = chkSyntex(query);
	var msgWait = "Verifying the query... <img src='/static/images/circle2.gif' height='20' width='20' >"

	if (vfy["flag"]) {
	    $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #vfy_msg").empty().append(vfy["err"]);
	    return false;
	}
	/* display waiting message */

	$j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #vfy_msg").empty().append(msgWait);
	    var sndData = {
		'cmd'       : 'verify',
		'query'	    : query,
		'bpath'	    : bpath,
		'stfile'    : stfile,
		'pdbfile'   : pdbfile,
		'trjFile[]' : trjFile,
	    }

        var request = $j.ajax({
        type: "POST",
        url: '/gui/verify_query/',
        cache: false,
        data: sndData,
        //async: false,
        });
		    
        request.done(function(Jdata) {
            var obj = $j.parseJSON(Jdata);
            $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #vfy_msg").empty().append("Verified!");
            $j("#dlg_selInfo #dlg_msg").empty().append(obj.selInfo);
	    $j("#dlg_selInfo").dialog({
		resizable: true,
		modal: true,
		height:500,
                width:600,
	    });

        });
          
        request.error(function(jqXHR, textStatus, errorThrown) {
            $j("#C_RDF2D #M_RDF2D #RDF2D2 #adjcenter_RDF2D #vfy_msg").empty().append(errorThrown);
        });
    });

     // Show sub menus
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #chk_RDF2D_lipid").live("click", function() {
	var chk = $j(this).prop('checked');
	if (chk == true) {
	    $cnt_modules++; // global variable to count all selected modules
	    cnt_RDF2D++; // count options in this module
	} else {
	    $cnt_modules--;
	    cnt_RDF2D--;
	    if ($cnt_modules < 0) {
		$cnt_modules = 0;
	    }
    	    if (cnt_RDF2D < 0) {
		cnt_RDF2D = 0;
	    }
	}
	$j("#C_RDF2D #M_RDF2D #RDF2D1 #cnt_RDF2D").empty().append(cnt_RDF2D); 	// updating currently selected options in this module
	$j("#cnt_modules").empty().append($cnt_modules);			// updating the count for all selected modules
	$j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid").toggle(this.checked);
    });

    // ---> define default query : select all atoms
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_query").val('segid MEMB');
    
    //---> Verification of selection query in RDF2D
    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid #define_lipid #pdb_btn_vfy").click(function(){
	var query = $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid #define_lipid #RDF2D_query").val().trim();
	var bpath = $j("#proot option:selected").text().trim();
	var stfile = $j("#stfile option:selected").text().trim();
	var pdbfile = $j("#pdbfile option:selected").text().trim();
	$j("#select_trj2 option").attr("selected", "selected"); 	// select all
	var trjFile = $j("#select_trj2").val();
	var vfy = chkSyntex(query);
	var msgWait = "Verifying the query... <img src='/static/images/circle2.gif' height='20' width='20' >"

	if (vfy["flag"]) {
	    $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid #define_lipid #vfy_msg").empty().append(vfy["err"]);
	    return false;
	}
	/* display waiting message */

	$j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid #define_lipid #vfy_msg").empty().append(msgWait);
	    var sndData = {
		'cmd'       : 'verify',
		'query'	    : query,
		'bpath'	    : bpath,
		'stfile'    : stfile,
		'pdbfile'   : pdbfile,
		'trjFile[]' : trjFile,
	    }

        var request = $j.ajax({
        type: "POST",
        url: '/gui/verify_query/',
        cache: false,
        data: sndData,
        //async: false,
        });
		    
        request.done(function(Jdata) {
            var obj = $j.parseJSON(Jdata);
            $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid #define_lipid #vfy_msg").empty().append("Verified!");
            $j("#dlg_selInfo #dlg_msg").empty().append(obj.selInfo);
	    $j("#dlg_selInfo").dialog({
		resizable: true,
		modal: true,
		height:500,
                width:600,
	    });

        });
          
        request.error(function(jqXHR, textStatus, errorThrown) {
            $j("#C_RDF2D #M_RDF2D #RDF2D2 #define_RDF2D #RDF2D_sub_lipid #define_lipid #vfy_msg").empty().append(errorThrown);
        });
    });

});
</script>

